                "--model-path", "/home/ubuntu/models/Qwen3-1.7B",
                "--compiled-model-path", "/home/ubuntu/traced_model/Qwen3-1.7B",

                "--torch-dtype", "bfloat16",
                "--tp-degree", "1",
                "--batch-size", "1",
                "--max-context-length", "1024",
                "--seq-len", "2048",
                "--check-accuracy-mode", "logit-matching",
                "--divergence-difference-tol", "0.005",
                "--tol-map", "{5: (1e-5, 0.02)}",
                "--enable-bucketing",
                "--top-k", "1",
                "--pad-token-id", "2",
                "--prompt", "I believe the meaning of life is",
                "--prompt", "The color of the sky is",
                "--compress-ratio", "0.8"
            ],
            "justMyCode": false

        }
    ]
}

